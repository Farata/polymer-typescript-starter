{
  "name": "polymer-starter",
  "version": "1.0.0-alpha.1",
  "private": true,
  "scripts": {
    "postinstall": "bower install && typings install",
    "clean": "gulp --cwd . --gulpfile tool/gulpfile-dev.ts clean",
    "start": "gulp --cwd . --gulpfile tool/gulpfile-dev.ts",
    "build": "gulp --cwd . --gulpfile tool/gulpfile-prod.ts",
    "test": "gulp --cwd . --gulpfile tool/gulpfile-test.ts",
    "preserve:dist": "npm run build",
    "serve:dist": "static dist -p 7777 -z"
  },
  "devDependencies": {
    "bower": "^1.7.7",
    "browser-sync": "^2.11.2",
    "del": "^2.2.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-gzip": "^1.2.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-shell": "^0.5.2",
    "gulp-typescript": "^2.12.0",
    "gulp-util": "^3.0.7",
    "node-static": "^0.7.7",
    "run-sequence": "^1.1.5",
    "ts-node": "^0.7.1",
    "typescript": "^1.8.9",
    "typings": "^0.7.11",
    "web-component-shards": "^1.1.3",
    "web-component-tester": "^4.2.2"
  }
}
